<!--pages/profile/childCpns/c-list-view/c-list-view.wxml-->
<view class='list-view'>
  <block wx:for="{{infos}}" wx:key="index">
    <view class='list-item'>
      <image class="image" src="/images/profile/{{item.icon}}"/>
      <text class="text">{{item.info}}</text>
    </view>
  </block>
</view>

